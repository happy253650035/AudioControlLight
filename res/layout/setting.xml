<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/set_bg" >

    <ImageView
        android:id="@+id/setList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:src="@drawable/set_list" />
    
    <Button
        android:id="@+id/radio1"
        android:layout_width="50dip"
        android:layout_height="50dip"
        android:layout_above="@+id/radio2"
        android:layout_alignParentLeft="true"
        android:background="@drawable/radio_up" />

    <Button
        android:id="@+id/radio2"
        android:layout_width="50dip"
        android:layout_height="50dip"
        android:layout_above="@+id/radio3"
        android:layout_alignParentLeft="true"
        android:background="@drawable/radio_up" />
    
    <Button
        android:id="@+id/radio3"
        android:layout_width="50dip"
        android:layout_height="50dip"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="69dp"
        android:background="@drawable/radio_up" />

    <ImageView
        android:id="@+id/setTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/back"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:src="@drawable/set_title" />

    <Button
        android:id="@+id/back"
        android:layout_width="60dip"
        android:layout_height="60dip"
        android:layout_alignParentLeft="true"
        android:background="@drawable/back" />

</RelativeLayout>